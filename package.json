  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "test": "run-s test:lint test:types test:serve",
    "test:ci": "CI=true npm run test",
    "test:lint": "eslint -c ./.eslintrc.js 'src/**/*.{ts,tsx}'",
