{
  "name": "blaketarter.com-2020",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prestart": "npm run build",
    "start": "next start",
    "preexport": "run-s clear build",
    "export": "next export",
    "clear": "rm -rf node_modules/.cache",
    "test": "run-s test:lint test:types test:unit:ci test:e2e:ci",
    "test:ci": "CI=true npm run test",
    "test:lint": "eslint -c ./.eslintrc.js 'src/**/*.{ts,tsx}'",
    "test:lint:fix": "npm run test:lint -- --fix",
    "test:unit": "jest --watch",
    "test:unit:ci": "jest --ci",
    "test:types": "tsc",
    "test:serve": "start-server-and-test test:serve:start http://localhost:5000 test:serve:verify",
    "test:serve:start": "npm run start -- -p 5000",
    "test:serve:verify": "echo 'ok'",
    "test:e2e": "cypress open",
    "test:e2e:ci": "start-server-and-test test:serve:start http://localhost:5000 test:e2e:verify",
    "test:e2e:verify": "docker run -t -v $PWD:/e2e -w /e2e cypress/included:4.12.1 open --project . --config baseUrl=http://host.docker.internal:5000",
    "test:e2e:ci:linux": "start-server-and-test test:serve:start http://localhost:5000 test:e2e:verify:linux",
    "test:e2e:verify:linux": "docker run -t -v $PWD:/e2e -w /e2e --add-host=host.docker.internal:172.17.0.1 cypress/included:4.12.1 open --project . --config baseUrl=http://host.docker.internal:5000"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "next": "13.0.4",
    "normalize.css": "8.0.1",
    "pattern.css": "1.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "7.0.6",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.6",
    "@types/classnames": "2.3.0",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.0",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "@types/testing-library__cypress": "5.0.8",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "babel-jest": "26.6.3",
    "cypress": "4.12.1",
    "cypress-plugin-snapshots": "1.4.4",
    "eslint": "7.28.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.23.2",
    "eslint-plugin-jest-dom": "3.9.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.10.2",
    "husky": "4.3.8",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "react-test-renderer": "17.0.2",
    "sass": "1.35.0",
    "start-server-and-test": "1.12.1",
    "typescript": "4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:ci"
    }
  }
}
