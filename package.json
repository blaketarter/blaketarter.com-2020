    "test:serve:start": "run-s build start",
    "test:serve:verify": "echo 'ok'"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "next": "^9.4.2",
    "normalize.css": "^8.0.1",
    "pattern.css": "^1.0.0",
