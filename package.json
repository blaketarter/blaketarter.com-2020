{
  "name": "blaketarter.com-2020",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prestart": "npm run build",
    "start": "next start",
    "preexport": "run-s clear build",
    "export": "next export",
    "clear": "rm -rf node_modules/.cache",
    "test": "run-s test:lint test:types test:e2e:ci",
    "test:ci": "CI=true npm run test",
    "test:lint": "eslint -c ./.eslintrc.js 'src/**/*.{ts,tsx}'",
    "test:lint:fix": "npm run test:lint -- --fix",
    "test:types": "tsc",
    "test:serve": "start-server-and-test test:serve:start http://localhost:5000 test:serve:verify",
    "test:serve:start": "npm run start -- -p 5000",
    "test:serve:verify": "echo 'ok'",
    "test:e2e": "cypress open",
    "test:e2e:ci": "CYPRESS_BASE_URL=http://localhost:5000 start-server-and-test test:serve:start http://localhost:5000 test:e2e:verify",
    "test:e2e:verify": "cypress run"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "next": "^9.5.2",
    "normalize.css": "^8.0.1",
    "pattern.css": "^1.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.1",
    "@types/classnames": "^2.2.10",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.47",
    "@types/react-dom": "^16.9.8",
    "@types/testing-library__cypress": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "cypress": "^4.12.1",
    "cypress-plugin-snapshots": "^1.4.4",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.0",
    "sass": "^1.26.10",
    "start-server-and-test": "^1.11.3",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:ci"
    }
  }
}
